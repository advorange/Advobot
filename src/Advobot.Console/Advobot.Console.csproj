<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Version>1.0.0</Version>
    <PackageId>Advobot.Console</PackageId>
    <Authors>Advorange</Authors>
    <Company>Advorange</Company>
    <Copyright>Copyright © 2019</Copyright>
    <Product>Advobot</Product>
    <NeutralLanguage>en</NeutralLanguage>
    <RepositoryUrl>https://github.com/advorange/Advobot</RepositoryUrl>
    <PackageProjectUrl>https://github.com/advorange/Advobot</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>Launches a console instance of Advobot.</Description>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Advobot.Core\Advobot.Core.csproj" />
    <ProjectReference Include="..\Commands\Advobot.Gacha\Advobot.Gacha.csproj" />
    <ProjectReference Include="..\Commands\Advobot.Invites\Advobot.Invites.csproj" />
    <ProjectReference Include="..\Commands\Advobot.Levels\Advobot.Levels.csproj" />
    <ProjectReference Include="..\Commands\Advobot.Settings\Advobot.Settings.csproj" />
    <ProjectReference Include="..\Commands\Advobot.Standard\Advobot.Standard.csproj" />
  </ItemGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <DefineConstants>TRACE</DefineConstants>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <Nullable>enable</Nullable>
    <LangVersion>preview</LangVersion>
  </PropertyGroup>

</Project>
